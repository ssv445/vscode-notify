{
  "name": "vscode-notify-desktop",
  "displayName": "VS Code Desktop Notify",
  "description": "Send desktop notifications from terminal that focus specific VS Code instances",
  "version": "0.2.1",
  "publisher": "ssv445",
  "author": "Shyam Verma",
  "license": "MIT",
  "homepage": "https://github.com/ssv445/vscode-notify-desktop#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ssv445/vscode-notify-desktop.git"
  },
  "bugs": {
    "url": "https://github.com/ssv445/vscode-notify-desktop/issues"
  },
  "keywords": [
    "vscode",
    "notification",
    "desktop",
    "terminal",
    "cli",
    "productivity",
    "workspace",
    "development"
  ],
  "engines": {
    "vscode": "^1.102.0",
    "node": ">=14.0.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {},
  "bin": {
    "vscode-notify-desktop": "./cli/vscode-notify-desktop.js"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/node-notifier": "^8.0.5",
    "@types/vscode": "^1.102.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "node-notifier": "^10.0.1"
  }
}
